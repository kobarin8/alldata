package com.internousdev.ecsite.action;

import java.util.Map;

import org.apache.struts2.interceptor.SessionAware;

import com.opensymphony.xwork2.ActionSupport;

public class DeleteItemAction  extends ActionSupport implements SessionAware{

	private String ItemNameDelete;
	public Map<String,Object> session;

	public String execute() {

		session.put("ItemNameDeleteKey", ItemNameDelete);


		return SUCCESS;

		}

	public String getItemNameDelete() {
		return ItemNameDelete;
	}

	public void setItemNameDelete(String itemNameDelete) {
		ItemNameDelete = itemNameDelete;
	}

	public Map<String, Object> getSession() {
		return session;
	}

	public void setSession(Map<String, Object> session) {
		this.session = session;
	}







}
